doctype html
html
    head
        title=`${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        h1 Custom Object Table
        a(class='link-update', href='/update-cobj') Add to this table
        table
            thead 
                tr
                    td Name
                    td Director
                    td Release Year
            tbody
                each movie in movies_results
                    tr
                        td #{movie.properties.name}
                        td #{movie.properties.director}
                        td #{movie.properties.release_year}